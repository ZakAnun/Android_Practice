<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zakli.practicethread">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/pthread_icon"
        android:label="@string/pthread_name"
        android:roundIcon="@mipmap/pthread_icon_round"
        android:supportsRtl="true"
        android:theme="@style/pthreadAppTheme">

        <activity android:name=".HIActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".HandlerThreadActivity"/>
        <activity android:name=".IntentServiceActivity"/>
        <service android:name=".DemoIntentService"/>
    </application>
</manifest>