<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zakli.practicelayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/playout_icon"
        android:label="@string/playout_name"
        android:roundIcon="@mipmap/playout_icon_round"
        android:supportsRtl="true"
        android:theme="@style/playoutAppTheme">

        <activity android:name=".LayoutActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".LayoutTimeConsumeActivity"/>
        <activity android:name=".LayoutDrawOptiActivity"/>

    </application>

</manifest>